AddKeysToAgent yes
# DevPod Start neo4j-cloud.devpod
Host neo4j-cloud.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  HostKeyAlgorithms rsa-sha2-256,rsa-sha2-512,ssh-rsa
  ProxyCommand "/usr/local/bin/devpod" ssh --stdio --context default --user codespace neo4j-cloud
  User codespace
# DevPod End neo4j-cloud.devpod
