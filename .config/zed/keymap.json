[
  {
    "context": "Workspace",
    "bindings": {
      "alt-j": "terminal_panel::ToggleFocus",
      "alt-shift-j": "terminal_panel::Toggle",
      "ctrl-enter": "terminal_panel::ToggleFocus",
      "alt-enter": "terminal_panel::Toggle"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-enter": "terminal_panel::ToggleFocus",
      "alt-enter": "terminal_panel::Toggle"
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "] d": "editor::GoToDiagnostic",
      "[ d": "editor::GoToPreviousDiagnostic",
      "alt-l": "pane::ActivateNextItem",
      "alt-h": "pane::ActivatePreviousItem",
      "alt->": "pane::ActivateNextItem",
      "alt-<": "pane::ActivatePreviousItem",
      "-": [
        "task::Spawn",
        {
          "task_name": "Yazi",
          "reveal_target": "center"
        }
      ],
      "space g g": [
        "task::Spawn",
        {
          "task_name": "start lazygit",
          "reveal_target": "center"
        }
      ],
      "space g f": [
        "task::Spawn",
        {
          "task_name": "start lazygit file log",
          "reveal_target": "center"
        }
      ],
      "s": "vim::PushReplaceWithRegister",
      "ctrl-enter": "terminal_panel::ToggleFocus",
      "alt-enter": "terminal_panel::Toggle"
    }
  }
]
