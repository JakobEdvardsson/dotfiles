[
  {
    "context": "Workspace",
    "bindings": {
      "alt-j": "workspace::NewCenterTerminal",
      "alt-shift-j": "workspace::NewCenterTerminal",
      "ctrl-enter": "workspace::NewCenterTerminal",
      "alt-enter": "workspace::NewCenterTerminal"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-enter": null,
      "alt-enter": null
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "] d": "editor::GoToDiagnostic",
      "[ d": "editor::GoToPreviousDiagnostic",
      "alt-l": "pane::ActivateNextItem",
      "alt-h": "pane::ActivatePreviousItem",
      "alt->": "pane::ActivateNextItem",
      "alt-<": "pane::ActivatePreviousItem",
      "space w v": "pane::SplitRight",
      "space w h": "workspace::ActivatePaneLeft",
      "space w left": "workspace::ActivatePaneLeft",
      "space w l": "workspace::ActivatePaneRight",
      "space w right": "workspace::ActivatePaneRight",
      "space w k": "workspace::ActivatePaneUp",
      "space w up": "workspace::ActivatePaneUp",
      "space w j": "workspace::ActivatePaneDown",
      "space w down": "workspace::ActivatePaneDown",
      "space w z": "workspace::ToggleZoom",
      "-": [
        "task::Spawn",
        {
          "task_name": "Yazi",
          "reveal_target": "center"
        }
      ],
      "space g g": [
        "task::Spawn",
        {
          "task_name": "start lazygit",
          "reveal_target": "center"
        }
      ],
      "space g f": [
        "task::Spawn",
        {
          "task_name": "start lazygit file log",
          "reveal_target": "center"
        }
      ],
      "s": "vim::PushReplaceWithRegister",
      "ctrl-enter": "workspace::NewCenterTerminal",
      "alt-enter": "workspace::NewCenterTerminal"
    }
  }
]
