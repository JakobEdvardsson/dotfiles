[
  {
    "label": "start lazygit",
    "command": "lazygit -p $ZED_WORKTREE_ROOT",
    "hide": "on_success",
    "shell": {
      "program": "sh"
    }
  },
  {
    "label": "start lazygit file log",
    "command": "lazygit",
    "args": ["-f", "\"$ZED_RELATIVE_FILE\""],
    "hide": "on_success",
    "shell": {
      "program": "sh"
    }
  },
  {
    "label": "Yazi",
    "command": "yazi_zed $ZED_FILE",
    "hide": "always",
    "use_new_terminal": true,
    "allow_concurrent_runs": false
  }
]
